# Environment variables template
# Copy to .env and fill in your own values. Do not commit secrets.

# Database (Supabase Postgres URI)
# Format:
# postgresql://postgres:YOUR_PASSWORD@db.YOUR_PROJECT_REF.supabase.co:5432/postgres?sslmode=require
DATABASE_URL=postgresql://postgres:CHANGE_ME@db.YOUR_PROJECT_REF.supabase.co:5432/postgres?sslmode=require

# Optional pooled connection (uncomment if needed)
# DATABASE_POOL_URL=postgresql://postgres:CHANGE_ME@pool.YOUR_PROJECT_REF.supabase.co:6543/postgres?sslmode=require

# Google Gemini
# Get from https://aistudio.google.com/ - keep secret
GEMINI_API_KEY=YOUR_GEMINI_KEY

# Clerk Publishable & Secret Keys
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_CHANGE_ME
CLERK_SECRET_KEY=sk_test_CHANGE_ME

# Clerk Webhooks (optional until you set up webhooks)
CLERK_WEBHOOK_SECRET_USER=whsec_user_CHANGE_ME
CLERK_WEBHOOK_SECRET_SESSION=whsec_session_CHANGE_ME

# Base URL (dev)
NEXT_PUBLIC_BASE_URL=http://localhost:3000

# Node environment
NODE_ENV=development
